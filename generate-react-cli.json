{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "utils/grc/templates/default/TemplateName.tsx",
        "style": "utils/grc/templates/default/TemplateName.module.scss",
        "test": "utils/grc/templates/default/TemplateName.test.tsx",
        "story": "utils/grc/templates/default/TemplateName.stories.tsx",
        "index": "utils/grc/templates/default/index.ts",
        "hook": "utils/grc/templates/default/useTemplateName.ts"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": false,
      "withIndex": true,
      "withHook": true
    },
    "layout": {
      "customTemplates": {
        "component": "utils/grc/templates/layout/TemplateName.tsx",
        "style": "utils/grc/templates/layout/TemplateName.module.scss",
        "test": "utils/grc/templates/layout/TemplateName.test.tsx",
        "story": "utils/grc/templates/layout/TemplateName.stories.tsx",
        "header": "utils/grc/templates/layout/Header.tsx",
        "footer": "utils/grc/templates/layout/Footer.tsx",
        "index": "utils/grc/templates/layout/index.ts"
      },
      "path": "src/layouts",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": false,
      "withHeader": true,
      "withFooter": true,
      "withIndex": true
    },
    "page": {
      "customTemplates": {
        "component": "utils/grc/templates/page/TemplateName.tsx"
      },
      "path": "pages",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    }
  }
}